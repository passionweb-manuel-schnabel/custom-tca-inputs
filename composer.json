{
    "name": "passionweb/custom-tca-inputs",
    "type": "typo3-cms-extension",
    "description": "Adds a custom TCA field with special rendering for JSON data. (TYPO3 CMS)",
    "version": "1.0.0",
    "homepage": "https://www.passionweb.de",
    "authors": [
        {
            "name": "Manuel Schnabel",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Passionweb\\CustomTcaInputs\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/custom-tca-inputs": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "custom_tca_inputs"
        }
    }
}
